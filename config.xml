<gap:splash src="splash/android/xhdpi.png" gap:platform="android" gap:density="xhdpi" />

<author href="http://tobepublicidad.com" email="agencia@tobe.mx">
Tobe Publicidad
</author>
<access origin="http://www.chocodiccionario.com/" subdomains="true"/>

